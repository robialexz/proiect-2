VITE_SUPABASE_URL=https://your-supabase-url.supabase.co
VITE_SUPABASE_ANON_KEY=your-supabase-anon-key

# Supabase service_role key (secret, do not commit .env)
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Configurări aplicație
VITE_APP_VERSION=1.0.0
VITE_APP_NAME=InventoryMaster
VITE_APP_DESCRIPTION=Aplicație pentru gestionarea inventarului și proiectelor
VITE_APP_AUTHOR=InventoryMaster Team

# Configurări regionale
VITE_APP_LANGUAGE=ro
VITE_APP_COUNTRY=RO
VITE_APP_CURRENCY=RON
VITE_APP_TIMEZONE=Europe/Bucharest
VITE_APP_DATE_FORMAT=DD.MM.YYYY
VITE_APP_TIME_FORMAT=HH:mm
VITE_APP_DATETIME_FORMAT=DD.MM.YYYY HH:mm
VITE_APP_DEFAULT_LOCALE=ro-RO

# Configurări pentru dezvoltare
VITE_DEV_MODE=true
VITE_ENABLE_TEST_ACCOUNTS=true
